@using SocialNetwork.Models;
@using SocialNetWork.Models;
@using SocialNetwork.ViewModels;
@model GetWallViewModel
@{
    ViewData["Title"] = "GetWall";
}


@foreach(var p in Model.posts)
{
    <div class="container bg-light">
        <p class="name float-right">
            @p.OwnerName
        </p>
        <p class="mt-3">
            @p.Text;
        </p>
    </div>

}